<h2><%= @article.title %></h2>
<p>Written by: <%= @article.user.display_name %></p>
<p>
  <%= @article.created_at %>
</p>
<p>
  <%= @article.content %>
</p>
<%= link_to "edit", edit_article_path, class: "btn btn-default btn-xs" %>
