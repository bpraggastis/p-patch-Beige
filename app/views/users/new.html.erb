<%= form_for @user do |f|%>
<ul>
  <li>Username <%= f.text_field :username , placeholder: "Username"%></li>
  <li>Email  <%= f.text_field :email, placeholder: "Email" %> </li>
  <li>Password <%= f.password_field :password %></li>
  <li><%= f.password_field :password_confirmation %> </li>
  <li> <%= f.submit %> </li>

</ul>
<% end %>
